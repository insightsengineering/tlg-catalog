body {
  background-color: #fff;
  margin: 1em auto;
  max-width: 800px;
  padding: 0 2em;
  line-height: 1.5;
}

.title, .author, .date {
  text-align: center;
}

#TOC {
  clear: both;
  margin: 0 0 10px 0;
  padding: 4px;
  border: 1px solid #CCCCCC;
  border-radius: 5px;
  background-color: #f6f6f6;
}
  #TOC .toctitle {
    font-weight: bold;
    font-size: 15px;
    margin-left: 5px;
  }

  #TOC ul {
    padding-left: 40px;
    margin-left: -1.5em;
    margin-top: 5px;
    margin-bottom: 5px;
  }
  #TOC ul ul {
    margin-left: -2em;
  }
